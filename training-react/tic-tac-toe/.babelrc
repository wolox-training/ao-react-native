{
  "presets": [
    "react-app"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "@components": "./src/app/components",
        "@screens": "./src/app/screens",
        "@redux": "./src/redux",
        "@services": "./src/services",
        "@consts": "./src/constants",
        "@utils": "./src/utils",
        "@validations": "./src/validations",
        "@assets": "./src/assets"
      }
    }]
  ]
}
